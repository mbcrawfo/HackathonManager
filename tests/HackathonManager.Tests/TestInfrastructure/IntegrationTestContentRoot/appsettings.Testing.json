{
  "AllowedHosts": "*",

  "ConsoleLog": {
    "Type": "Text",
    "ConsoleLogLevel": "Verbose"
  },

  "DatabaseLogging": {
    "EnableDetailedErrors": true,
    "EnableSensitiveDataLogging": true
  },

  "EnableIntegratedSpa": false,
  "EnableStartupMigration": false,

  "FileLog": {
    "Enabled": false
  },

  "OpenTelemetryL": {
    "AllExporters": {
      "Enabled": false
    }
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },

  "ServiceName": "hackathon-manager-web",

  "RequestLogging": {
    "LogBody": true
  }
}

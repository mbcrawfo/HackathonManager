{
  "ConnectionStrings": {
    "HackathonDb": "Host=localhost;Port=5432;Username=postgres;Password=postgres;Database=hackathon"
  },

  "ConsoleLog": {
    "Type": "Text",
    "ConsoleLogLevel": "Information"
  },

  "Log": {
    "EnableOpenTelemetryLogging": true,
    "OpenTelemetryLogLevel": "Debug",
    "OtlpEndpoint": "http://localhost:5341/ingest/otlp/v1/logs",
    "OtlpProtocol": "HttpProtobuf"
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    }
  },

  "RequestLogging":  {
    "ContentTypes": ["application/json", "application/x-www-form-urlencoded", "multipart/form-data"]
  },

  "Trace": {
    "Enabled": true,
    "OtlpEndpoint": "http://localhost:5341/ingest/otlp/v1/traces",
    "OtlpProtocol": "HttpProtobuf"
  }
}
